<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.MemberMapper">
    <insert id="write" parameterType="com.example.demo.dto.BoardDTO">
        insert into board
        values (#{bno}, #{title}, #{content}, #{writer}, #{regdate}, #{viewcnt})
    </insert>
    <select id="getBoard" parameterType="com.example.demo.dto.BoardDTO" resultType="com.example.demo.dto.BoardDTO">
        select *
        from board
        where bno = #{bno}
          and title = #{title}
          and content = #{content}
          and writer = #{writer}
          and regdate = #{regdate}
    </select>
</mapper>